<h1>NYC Core Framework Customization</h1>
<h2>Form</h2>
<form method="POST" action="/compiler">
  <div class="form-group">
    <label for="primary">Primary</label>
    <input type="text" class="form-control" id="primary" name="primary" aria-describedby="primary" placeholder="Enter Primary" >
  </div>
  <div class="form-group">
    <label for="secondary">Secondary</label>
    <input type="text" class="form-control" id="secondary" name="secondary" aria-describedby="secondary" placeholder="Enter Secondary" >
  </div>
  <div class="form-group">
    <label for="info">Info</label>
    <input type="text" class="form-control" id="info" name="info" aria-describedby="info" placeholder="Enter Info" >
  </div>
  <div class="form-group">
    <label for="light">Light</label>
    <input type="text" class="form-control" id="light" name="light" aria-describedby="light" placeholder="Enter Light" >
  </div>
  <div class="form-group">
    <label for="dark">Dark</label>
    <input type="text" class="form-control" id="dark" name="dark" aria-describedby="dark" placeholder="Enter Dark" >
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
